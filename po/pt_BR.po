#~ "\n"
#~ "  EXPRESSION ::= SYMBOL | EXPRESSION ( `+' | `|' ) SYMBOL\n"
#~ "\n"
#~ msgstr ""
#~ "Utilização: %s [OPÇÕES] AÇÕES\n"
#~ "\n"
#~ "Opções:\n"
#~ " -d, -display DISPLAY                Conecta-se ao servidor X\n"
